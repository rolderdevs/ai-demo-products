{
  "name": "ai-demo-products",
  "version": "v0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rsbuild build",
    "dev": "concurrently \"bun run server\" \"bun run frontend\"",
    "server": "bun run --watch src/infrastructure/web-adapters/elysia/server.ts",
    "frontend": "rsbuild dev -c src/presentation/react/rsbuild.config.ts",
    "check": "biome check --write"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@hono/node-server": "^1.19.2",
    "elysia": "^1.4.4",
    "hono": "^4.9.7",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.3",
    "@rsbuild/core": "^1.5.4",
    "@rsbuild/plugin-react": "^1.4.0",
    "@types/bun": "^1.2.21",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "concurrently": "^9.2.1",
    "typescript": "^5.9.2"
  },
  "trustedDependencies": [
    "core-js",
    "esbuild"
  ]
}
